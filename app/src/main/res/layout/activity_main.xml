    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/happy48"
        android:background="@android:color/transparent"
        android:layout_marginLeft="25dp"
        android:layout_marginStart="25dp"
        android:layout_marginTop="140dp"
        android:id="@+id/happy"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/sad48"
        android:background="@android:color/transparent"
        android:layout_alignTop="@+id/happy"
        android:layout_toRightOf="@+id/happy"
        android:layout_toEndOf="@+id/happy"
        android:layout_marginLeft="31dp"
        android:layout_marginStart="31dp"
        android:id="@+id/sad" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/meh48"
        android:layout_alignTop="@+id/sad"
        android:layout_toRightOf="@+id/sad"
        android:layout_toEndOf="@+id/sad"
        android:layout_marginLeft="33dp"
        android:background="@android:color/transparent"
        android:layout_marginStart="33dp"
        android:id="@+id/meh" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/excited48"
        android:layout_alignTop="@+id/meh"
        android:layout_toRightOf="@+id/meh"
        android:background="@android:color/transparent"
        android:layout_toEndOf="@+id/meh"
        android:layout_marginLeft="28dp"
        android:layout_marginStart="28dp"
        android:id="@+id/excited" />

    <TextView
        android:text="Happy"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/happy"
        android:layout_alignLeft="@+id/happy"
        android:layout_alignStart="@+id/happy"
        android:layout_marginTop="10dp"
        android:id="@+id/happytext" />

    <TextView
        android:text="Sad"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/sadtext"
        android:layout_alignBaseline="@+id/happytext"
        android:layout_alignBottom="@+id/happytext"
        android:layout_toLeftOf="@+id/meh"
        android:layout_toStartOf="@+id/meh"
        android:layout_marginRight="11dp"
        android:layout_marginEnd="11dp" />

    <TextView
        android:text="Excited"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/mehtext"
        android:layout_alignLeft="@+id/excited"
        android:layout_alignStart="@+id/excited"
        android:id="@+id/excitedtext" />

    <TextView
        android:text="Meh"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/mehtext"
        android:layout_alignBaseline="@+id/sadtext"
        android:layout_alignBottom="@+id/sadtext"
        android:layout_alignLeft="@+id/meh"
        android:layout_alignStart="@+id/meh" />

 <Button
     android:text="Date"
     android:layout_width="wrap_content"
     android:layout_height="wrap_content"
     android:layout_alignParentBottom="true"
     android:layout_alignParentLeft="true"
     android:layout_alignParentStart="true"
     android:layout_marginBottom="101dp"
     android:id="@+id/date" />

 <Button
     android:text="Time"
     android:layout_width="wrap_content"
     android:layout_height="wrap_content"
     android:layout_alignTop="@+id/date"
     android:layout_alignParentRight="true"
     android:layout_alignParentEnd="true"
     android:id="@+id/time" />

    <Button
        android:text="stats"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/stat"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true" />

</RelativeLayout>
